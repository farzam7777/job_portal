<!DOCTYPE html>
<html>
  <head>
    <title>JobPortal</title>
    <%= csrf_meta_tags %>

    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'css/colors/green', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="wrapper">
      <%= render 'header' %>
        <%= render "notifications" %>
        <%= yield %>
      <%= render 'footer' %>
    </div>
        
    <!-- Scripts
    ================================================== -->
    <%= javascript_include_tag 'jquery-2.1.3.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'custom.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.superfish.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.themepunch.tools.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.themepunch.revolution.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.themepunch.showbizpro.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.flexslider-min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'chosen.jquery.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.magnific-popup.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'waypoints.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.counterup.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.jpanelmenu.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'stacktable.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'headroom.min.js', 'data-turbolinks-track': 'reload' %>
  </body>
</html>
